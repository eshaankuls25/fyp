#!/bin/bash
#
# Script to set project aliases
#

echo "To copy code from python scripts to /usr/share/setoolkit (from $HOME/code/Python), press 'y'. Otherwise press another key.";
read option

if [ "$option" = 'y' ]; then
sudo cp ~/set/code/Python/*.py /usr/share/setoolkit
fi

gnome-terminal -e runpktcap
runextractdata